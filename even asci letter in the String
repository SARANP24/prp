import java.util.*;
public class a
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        String s1=s.nextLine();
        for(int i=0;i<s1.length();i++)
    {
        char ch=s1.charAt(i);
         if((char)(ch)%2 == 0)
         {
             System.out.print(ch);
         }
    }
    }
}
